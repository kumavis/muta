{
  "name": "muta",
  "version": "0.2.2",
  "description": "Transactions for object mutations",
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "pretest": "depcheck --ignores=babel-preset-es2015,airtap && standard src/*.js test/*.js",
    "test": "npm run pretest && nyc -r text -r html tape test/*.js | faucet && nyc report",
    "test-browser": "npm run pretest && npm run build && test/browser.sh && npm run source",
    "build": "rm -rf lib && babel --presets es2015 src -d lib",
    "source": "rm -rf lib && ln -s src lib",
    "prepublish": "npm run build",
    "publish": "npm run source"
  },
  "keywords": [
    "proxy",
    "mutation",
    "state",
    "commit"
  ],
  "author": "Matt Bell <mappum@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "airtap": "^0.1.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "depcheck": "^0.6.9",
    "faucet": "0.0.1",
    "nyc": "^12.0.2",
    "standard": "^11.0.1",
    "tape": "^4.9.1"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mappum/muta.git"
  },
  "bugs": {
    "url": "https://github.com/mappum/muta/issues"
  },
  "homepage": "https://github.com/mappum/muta#readme"
}
